<h2>Available drones</h2>
{{#each pr}}
    <p><strong>Name: </strong>{{this.name}}</p>
    <p><strong>Number of propellers: </strong>{{this.propellers}}</p>
    <p><strong>Maximum speed: </strong>{{this.maxSpeed}}m/s</p>
    <a href="/drones/{{this._id}}/edit"><button>Edit</button></a>
    <form action="/drones/{{this._id}}/delete" method="POST">
        <input type="submit" value="Delete" />
    </form>
    <br />
{{/each}}
<a href="/drones/create"><button>Add drone</button></a>